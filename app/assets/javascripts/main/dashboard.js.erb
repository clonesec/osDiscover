  $(document).ready(function(){  
    $("a.tab").click(function (e) {  
        $(".active").removeClass("active");  
        $(this).addClass("active");  
        $(".content").hide();
        var content_show = $(this).attr("store");  		
        $("#"+content_show).show();  
		e.preventDefault();
    });
	
	
	//Boxes
	$(".box").click(function(e) {
		$(".box").removeAttr("id");
		$(this).attr("id", "active-box");
		e.preventDefault();
	});
  });  
  
